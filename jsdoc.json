{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [
            "node_modules",
            "lib/node_modules"
        ],
        "excludePatterns": "(^|\\/|\\\\|node_modules)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "systemName": "Netstats CRON",
        "monospaceLinks": false,
        "dateFormat": "YYYY/MM/DD",
        "navType": "inline",
        "theme": "slate",
        "linenums": true,
        "footer": "",
        "inverseNav": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./documentation/",
        "recurse": true
    }
}
