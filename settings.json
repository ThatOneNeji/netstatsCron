{
    "amqp": {
        "server": "192.168.72.250",
        "user": "netstatsNodeCron",
        "password": "netstatsNodeCron",
        "queuename": "netstats/cron/"
    },
    "snmp": {
        "community": "asuran",
        "port": 161
    },    
    "logger": {
        "appenders": {
            "out": {
                "type": "console"
            },
            "task": {
                "type": "file",
                "filename": "logs/netstatsNodeCron.log",
                "maxLogSize": 1048576,
                "backups": 10
            }
        },
        "categories": {
            "default": {
                "appenders": ["out", "task"],
                "level": "debug"
            },
            "task": {
                "appenders": ["task"],
                "level": "error"
            }
        }
    }
}